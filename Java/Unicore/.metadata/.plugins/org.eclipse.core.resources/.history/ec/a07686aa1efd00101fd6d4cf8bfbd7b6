package console.commands;

import main.Base;
import console.ConsoleCommand;

public class Echo extends ConsoleCommand {
	public Echo(Base b, String[] args) {
		super(b, args);
	}
	
	public void run() {

		base.getConsole().printAt("Enter text: ", false);
		
		
		
		base.getConsoleHead().informExeEnd();
	}
}
